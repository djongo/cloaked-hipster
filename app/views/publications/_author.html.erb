<li id="author_<%= f.object[:id] %>" class="child">
  <span class="handle">[drag]</span>
  <%= f.text_field :name, :size => 20 %>
  <%= f.text_field :email, :size => 20 %>
  <%= f.collection_select :country_team_id, CountryTeam.order(:name), :id, :name, options = { :prompt => "Select country team" }, html_options = { :size => "1" } %>
  <%= f.collection_select :focus_group_id, FocusGroup.order(:name), :id, :name, options = { :prompt => "Select focus group" }, html_options = { :size => "1" } %>
  <%= remove_child_link 'Remove author', f %>
</li>
